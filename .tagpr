# tagpr configuration
# https://github.com/Songmu/tagpr

[tagpr]
# Release branch
releaseBranch = main

# Version files to update
versionFile = package.json, wp-blogcard.php, readme.txt

# Use v prefix for tags (e.g., v1.0.0)
vPrefix = true

# Create GitHub release on tag
release = true

# Commit message template
#tmpl = Release {{.NextVersion}}
